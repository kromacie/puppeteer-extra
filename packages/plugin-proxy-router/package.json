  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^20.10.5",
    "@types/puppeteer": "*",
    "ava": "2.4.0",
    "copyfiles": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "playwright-core": "1.24.2",
    "prettier": "^2.7.1",
